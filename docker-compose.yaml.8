version: "3"

services:
  clef-1:
    container_name: clef-1
    image: ethersphere/clef:latest
    restart: unless-stopped
    environment:
      - CLEF_CHAINID=5
    volumes:
      - ./clef/01:/app/data
    command: full

  node-1:
    container_name: node-1
    image: ethersphere/bee:0.6.2
    restart: unless-stopped
    environment:
      - BEE_FULL_NODE=true
      - BEE_API_ADDR=:1633
      - BEE_P2P_ADDR=:1634
      - BEE_NAT_ADDR=IP주소:1634
      - BEE_DEBUG_API_ADDR=:1635
      - BEE_CLEF_SIGNER_ENDPOINT=http://clef-1:8550
      - BEE_DEBUG_API_ENABLE=true
      - BEE_CLEF_SIGNER_ENABLE=true
      - BEE_PASSWORD=bee01
      - BEE_SWAP_ENDPOINT=https://rpc.slock.it/goerli
      - BEE_WELCOME_MESSAGE=Hello from Korean Bees bzz bzz GGAZUA
    ports:
      - "${API_ADDR:-1633}${BEE_API_ADDR:-:1633}"
      - "${P2P_ADDR:-1634}${BEE_P2P_ADDR:-:1634}"
      - "${DEBUG_API_ADDR:-127.0.0.1:1635}${BEE_DEBUG_API_ADDR:-:1635}"
    volumes:
      - ./node/01:/home/bee/.bee
    command: start
    depends_on:
      - clef-1

